<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/assignment/db" VERSION="20100905" COMMENT="XMLDB file for Moodle mod/assignment"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="reservations" COMMENT="Individual information about reservations" NEXT="equipment">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" SEQUENCE="true" NOTNULL="true" NEXT="equipment_id"/>
        <FIELD NAME="equipment_id" TYPE="int" NOTNULL="true" NEXT="date" PREVIOUS="id" SEQUENCE="false"/>
        <FIELD NAME="date" TYPE="int" NOTNULL="true" NEXT="end_date" PREVIOUS="equipment_id" SEQUENCE="false"/>
        <FIELD NAME="end_date" TYPE="int" NOTNULL="true" NEXT="duration" PREVIOUS="date" SEQUENCE="false"/>
        <FIELD NAME="duration" TYPE="int" NOTNULL="true" NEXT="owner_id" PREVIOUS="end_date" SEQUENCE="false"/>
        <FIELD NAME="owner_id" TYPE="int" NOTNULL="true" NEXT="course" PREVIOUS="duration" SEQUENCE="false"/>
        <FIELD NAME="course" TYPE="int" NOTNULL="false" NEXT="created_at" PREVIOUS="owner_id" SEQUENCE="false"/>
        <FIELD NAME="created_at" TYPE="int" NOTNULL="false" PREVIOUS="course" SEQUENCE="false"/>
      </FIELDS>
        <KEYS>
          <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>

    <TABLE NAME="equipment" COMMENT="Information for pieces of equipment" PREVIOUS="reservations" NEXT="contents">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" NOTNULL="true" NEXT="name" SEQUENCE="true"/>
        <FIELD NAME="name" LENGTH="50" TYPE="char" NOTNULL="true" PREVIOUS="id" NEXT="description" SEQUENCE="false" />
        <FIELD NAME="description" LENGTH="300" TYPE="char" NOTNULL="true" PREVIOUS="name" NEXT="code" SEQUENCE="false" />
        <FIELD NAME="code" LENGTH="1200" TYPE="char" NOTNULL="true" PREVIOUS="description" SEQUENCE="false" />
      </FIELDS>
        <KEYS>
          <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>

    <TABLE NAME="contents" COMMENT="Experiment contents" PREVIOUS="equipment" NEXT="experiments">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" NOTNULL="true" NEXT="name" SEQUENCE="true"/>
        <FIELD NAME="name" LENGTH="50" TYPE="char" NOTNULL="true" PREVIOUS="id" NEXT="filepath" SEQUENCE="false" />
        <FIELD NAME="filepath" LENGTH="300" TYPE="char" NOTNULL="true" PREVIOUS="name" NEXT="experiment_id" SEQUENCE="false" />
        <FIELD NAME="experiment_id" TYPE="int" NOTNULL="true" PREVIOUS="filepath" NEXT="type" SEQUENCE="false" />
        <FIELD NAME="type" TYPE="char" LENGTH="100" NOTNULL="true" PREVIOUS="experiment_id" SEQUENCE="false" />
      </FIELDS>
        <KEYS>
          <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>

    <TABLE NAME="experiments" COMMENT="Experiments" PREVIOUS="contents" NEXT="laboratories">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" NOTNULL="true" NEXT="name" SEQUENCE="true"/>
        <FIELD NAME="name" LENGTH="50" TYPE="char" NOTNULL="true" PREVIOUS="id" NEXT="html" SEQUENCE="false" />
        <FIELD NAME="html" LENGTH="500" TYPE="char" NOTNULL="true" PREVIOUS="name" NEXT="laboratory_id" SEQUENCE="false" />
        <FIELD NAME="laboratory_id" TYPE="int" NOTNULL="true" PREVIOUS="html" NEXT="description" SEQUENCE="false" />
        <FIELD NAME="description" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="laboratory_id"/>
      </FIELDS>
        <KEYS>
          <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>

    <TABLE NAME="laboratories" COMMENT="Labs" PREVIOUS="experiments">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" NOTNULL="true" NEXT="name" SEQUENCE="true"/>
        <FIELD NAME="name" LENGTH="50" TYPE="char" NOTNULL="true" PREVIOUS="id" NEXT="description" SEQUENCE="false" />
        <FIELD NAME="description" LENGTH="300" TYPE="char" NOTNULL="true" PREVIOUS="name" SEQUENCE="false" />
      </FIELDS>
        <KEYS>
          <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
